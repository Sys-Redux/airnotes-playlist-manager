{
  "name": "graph-notes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "prisma generate && concurrently -n gql,prisma,next -c blue,yellow,green \"pnpm codegen:watch\" \"pnpm prisma:watch\" \"next dev\"",
    "build": "prisma generate && pnpm codegen && next build",
    "start": "next start",
    "lint": "eslint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "prisma:watch": "nodemon --watch prisma/schema.prisma --ext prisma --exec \"prisma generate\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/next": "^4.1.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.6",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "graphql-tag": "^2.12.6",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-resolvers": "^5.1.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "nodemon": "^3.1.11",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
